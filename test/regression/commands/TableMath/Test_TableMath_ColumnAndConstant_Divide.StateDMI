# Test dividing table column by constant
StartLog(LogFile="Results/Test_TableMath_ColumnAndConstant_Divide.TSTool.log")
RemoveFile(InputFile="Results/Test_TableMath_ColumnAndConstant_Divide_out.dv",IfNotFound=Ignore)
# Read previously generated time series data (from TSTool)
ReadTableFromDelimitedFile(TableID="Table1",InputFile="Data\timeseries.csv",DateTimeColumns="Date",DoubleColumns="ts1,ts2")
# Perform math on the table
TableMath(TableID="Table1",Input1="ts1",Operator="/",Input2="2",Output="result")
# Uncomment the following command to regenerate expected results.
# WriteTableToDelimitedFile(TableID="Table1",OutputFile="ExpectedResults/Test_TableMath_ColumnAndConstant_Divide_out.csv")
WriteTableToDelimitedFile(TableID="Table1",OutputFile="Results/Test_TableMath_ColumnAndConstant_Divide_out.csv")
CompareFiles(InputFile1="ExpectedResults/Test_TableMath_ColumnAndConstant_Divide_out.csv",InputFile2="Results/Test_TableMath_ColumnAndConstant_Divide_out.csv",IfDifferent=Warn)
